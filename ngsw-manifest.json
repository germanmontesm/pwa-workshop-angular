{
  "static.ignore": [
    "^\/assets\/ignore.*$"
  ],
  "routing": {
    "index": "/index.html",
    "routes": {
      "/": {
        "prefix": false
      },
      "/some-routes-group-prefix": {
        "prefix": true
      },
      "/some-routes-group-prefix-no-files": {
        "prefix": true,
        "onlyWithoutExtension": true
      },
      "/some/specific-route": {
        "prefix": false
      },
      "^/regex/.*/route$": {
        "match": "regex"
      }
    }
  }
}
